{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "keyVaultName": {
            "type": "string"
        },
        "AccessPolicyObjectId": {
          "type": "string"
        },
        "AccessPolicyObjectId2": {
          "type": "string"
        }
    },
    "variables": {
        "webAppAccessPolicy": [
            {
              "objectId": "[parameters('AccessPolicyObjectId')]",
              "tenantId": "[subscription().tenantId]",
              "permissions": {
                "keys": [],
                "secrets": [
                    "Get",
                    "List",
                    "Set",
                    "Delete",
                    "Recover",
                    "Backup",
                    "Restore"
                ],
                "certificates": []
            }
            },{
                "objectId": "[parameters('AccessPolicyObjectId2')]",
                "tenantId": "[subscription().tenantId]",
                "permissions": {
                  "keys": [],
                  "secrets": [
                      "Get",
                      "List",
                      "Set",
                      "Delete",
                      "Recover",
                      "Backup",
                      "Restore"
                  ],
                  "certificates": []
              }
              }
          ]
    },
    "resources": [
        {
            "type": "Microsoft.KeyVault/vaults",
            "name": "[parameters('keyVaultName')]",
            "apiVersion": "2015-06-01",
            "location": "[resourceGroup().location]",
            "properties": {
                "enabledForTemplateDeployment": "true",
                "accessPolicies": "[variables('webAppAccessPolicy')]",
                "tenantId": "[subscription().tenantId]",
                "sku": {
                    "name": "Standard",
                    "family": "A"
                }
            }
        }
    ],
    "outputs": {}
}