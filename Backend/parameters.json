{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "templatesBaseUri": {
            "value": "https://raw.githubusercontent.com/DFE-Digital/login.dfe.infrastructure/master/"
        },
        "appServicePlanName": {
            "value": "__AppServicePlanNameGreen__"
        },
        "appServicePlanSku": {
            "value": {
                "name": "S1",
                "tier": "Standard",
                "size": "S1",
                "family": "S",
                "capacity": 1
            }
        },
        "appServicePlanOS": {
            "value": "app"
        },
        "appServicePlanIsLinux": {
            "value": false
        },
        "keyvaultCertificateNames": {
            "value": [
                "__signinProxyCertName__",
                "__signinCertName__"
            ]
        },
        "keyVaultResourceGroup": {
            "value": "__keyVaultResourceGroup__"
        }
    }
}

